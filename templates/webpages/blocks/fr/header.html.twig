<header id="header">
<h1>En-tÃªte du site</h1>
</header>

<nav>
	<ul id="nav">
		{% for item in menus.findOneBy({'pos': 0}).menulinks | sort((a, b) => a.orderlink <=> b.orderlink) %}
			{% if item.page is not null %}
				<li>
					<a href="{{ path('web_page', {page_slug: item.page.pageurl}) }}">{{ item.page.pagename }}</a>
				</li>
			{% elseif item.post is not null %}
				<li>
					<a href="{{ path('web_post', {post_url: item.post.posturl}) }}">{{ item.page.postname }}</a>
				</li>
			{% else %}
				<li>
					<a href="{{ item.cuslink }}" target="_blank" rel="noopener">{{ item.cusname }}</a>
				</li>
			{% endif %}
		{% endfor %}
	</ul>
</nav>